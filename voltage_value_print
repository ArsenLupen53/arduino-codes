int value=0;
float voltage = 0.0;

void setup() {
  // put your setup code here, to run once:
  Serial.begin(9600);
}

void loop() {
  // put your main code here, to run repeatedly:
  value = analogRead(A0);
  Serial.print("value= ");
  Serial.print(value);

  voltage = value * (5000.0/1023);
  Serial.print(". voltage = ");
  Serial.println(voltage);
}
